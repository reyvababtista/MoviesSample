<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/initialFragment">
    <fragment
        android:id="@+id/initialFragment"
        android:name="com.rey.movies.presentation.fragment.InitialFragment"
        android:label="InitialFragment">
        <action
            android:id="@+id/action_initialFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.rey.list.presentation.activity.MainActivity"
        android:label="activity_main"
        app:moduleName="list"
        tools:layout="@layout/activity_main" />
</navigation>